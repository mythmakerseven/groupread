{
  "name": "groupread",
  "version": "0.0.1",
  "description": "Self-hosted book club platform",
  "main": "index.js",
  "scripts": {
    "start": "yarn node build/server/index.js",
    "dev:server": "yarn tsc-watch --project server/tsconfig.json --onSuccess \"yarn start\"",
    "dev:ui": "webpack serve --mode=development",
    "test:jest": "NODE_ENV=test jest --config ./server/jest.config.ts --verbose --runInBand",
    "test:component": "npx cypress run-ct",
    "start:cypress": "NODE_ENV=test yarn start",
    "test:e2e": "cypress run",
    "lint": "eslint .",
    "build:server": "yarn tsc --build server/tsconfig.json",
    "build:ui": "webpack --mode=production",
    "start:full": "yarn build:server && yarn build:ui && yarn start"
  },
